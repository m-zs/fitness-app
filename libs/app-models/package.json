{
  "name": "@fitness-app/app-models",
  "version": "0.0.1",
  "private": true,
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./package.json": "./package.json"
  },
  "dependencies": {},
  "nx": {
    "targets": {
      "migrate": {
        "executor": "nx:run-commands",
        "options": {
          "command": "prisma migrate dev --schema=libs/app-models/src/lib/prisma/schema.prisma"
        }
      },
      "generate": {
        "executor": "nx:run-commands",
        "options": {
          "command": "prisma generate --schema=libs/app-models/src/lib/prisma/schema.prisma"
        }
      },
      "test": {
        "options": {
          "passWithNoTests": true
        }
      }
    }
  }
}
